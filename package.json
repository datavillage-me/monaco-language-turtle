  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "jest": {},
  "devDependencies": {
    "antlr4ts-cli": "^0.5.0-alpha.3"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3",
    "monaco-editor": "^0.21.2",
    "rdflib": "^2.1.3"
  }
}
