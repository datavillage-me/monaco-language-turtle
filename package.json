{
  "name": "monaco-language-turtle",
  "version": "0.0.1",
  "description": "Turtle support for Monaco Editor",
  "author": {
    "name": "Philippe Duchesne",
    "email": "pduchesne@gmail.com"
  },
  "scripts": {
    "build": "yarn clean && yarn build-tsc",
    "build-tsc": "tsc -b .",
